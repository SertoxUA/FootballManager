<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ua.sertox.football">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        android:screenOrientation="landscape">
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Team.Vorskla" />
        <activity android:name=".Team.Shakhtar" />
        <activity android:name=".Team.Dynamo" />
        <activity android:name=".Team.Veres" />
        <activity android:name=".Team.Zarya"></activity>
    </application>

</manifest>