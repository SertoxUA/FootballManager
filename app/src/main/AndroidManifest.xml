<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ua.sertox.football">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".StartActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TemplateTeamActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".OldActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".TeamSelectionActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".MainActivity" />
        <activity android:name=".expand.ExpandActivity" />
        <activity android:name=".multicheck.MultiCheckActivity" />
        <activity android:name=".multitype.MultiTypeActivity" />
        <activity android:name=".multitypeandcheck.MultiTypeCheckGenreActivity" />
        <activity android:name=".singlecheck.SingleCheckActivity" />
    </application>

</manifest>